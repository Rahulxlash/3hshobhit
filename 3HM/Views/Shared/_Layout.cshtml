                                    <li class="@(ViewContext.RouteData.Values["action"].ToString() =="Products" ? "active":"" )">@Html.ActionLink("Products", "Products", "Home")</li>
                                    <li class="@(ViewContext.RouteData.Values["action"].ToString()=="ClientsPartners" ? "active":"" )">@Html.ActionLink("Clients", "ClientsPartners", "Home")</li>
                                    @*<li class="@(ViewContext.RouteData.Values["action"].ToString() == "Business" ? "active" : "")">@Html.ActionLink("Business Opportunities", "Business", "Home")</li>*@
